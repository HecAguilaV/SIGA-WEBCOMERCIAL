{
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/comercial/:match*",
      "destination": "https://siga-backend-production.up.railway.app/api/comercial/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}