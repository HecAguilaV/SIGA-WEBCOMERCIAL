{
  "name": "siga-web-comercial",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:clean": "lsof -ti:5173 | xargs kill -9 2>/dev/null || true && rm -rf node_modules/.vite && pnpm run dev",
    "sync:openapi": "mkdir -p public/docs/api && cp docs/api/openapi.yaml public/docs/api/openapi.yaml",
    "prebuild": "mkdir -p public/docs/api && cp docs/api/openapi.yaml public/docs/api/openapi.yaml",
    "build": "vite build",
    "preview": "vite preview",
    "test": "karma start karma.conf.cjs",
    "test:single": "karma start karma.conf.cjs --single-run"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "bootstrap": "^5.3.3",
    "phosphor-react": "^1.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "recharts": "^3.3.0",
    "swagger-ui-react": "^5.30.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-loader": "^9.2.0",
    "jasmine-core": "^5.3.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-webpack": "^5.0.0",
    "null-loader": "^4.0.1",
    "vite": "^5.4.10",
    "webpack": "^5.95.0"
  }
}
